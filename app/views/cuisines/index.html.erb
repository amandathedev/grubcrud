<h1>GrubCRUD</h1>
<h3>Explore our cuisines</h3>
<% @cuisines.each do |cuisine|%>
  <p align="left"><%= link_to cuisine.name, cuisine_path(cuisine) %></p>
  <%= link_to image_tag("#{cuisine.img_url}", :size => '400x200'), cuisine_path(cuisine) %>
  <!-- <img src = "https://loremflickr.com/<%=cuisine.img_url%>" width = "200" height = "100"> -->
<% end %>
